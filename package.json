{
  "name": "katy-decorah",
  "version": "0.0.0",
  "description": "Katy DeCorah's personal website",
  "scripts": {
    "test": "tape test/*.test.js | tap-min && npm run lint-md && npm run test-content",
    "test-content": "extra-eyes {adventures,code,epicurean,notes}/_posts --dict=katydecorah.dic",
    "lint-md": "remark {adventures,code,epicurean,notes}/_posts",
    "send-images": "node bin/appropriate-images.js",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "pretty-quick": "pretty-quick"
  },
  "devDependencies": {
    "@double-great/remark-lint-alt-text": "^0.3.0",
    "@mapbox/remark-lint-link-text": "^0.5.0",
    "eslint": "^7.4.0",
    "extra-eyes": "^1.3.0",
    "feather-icons": "^4.28.0",
    "husky": "^4.2.5",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "remark-cli": "^8.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-frontmatter-validator": "^0.4.0",
    "remark-lint-heading-increment": "^2.0.1",
    "tap-min": "^2.0.0",
    "tape": "^5.0.1"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "katydecorah.dic": [
      "./scripts/alpha"
    ]
  },
  "main": "index.js",
  "bin": {
    "update-icons": "bin/icons.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/katydecorah.github.io.git"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katydecorah/katydecorah.github.io/issues"
  },
  "homepage": "https://github.com/katydecorah/katydecorah.github.io#readme"
}
