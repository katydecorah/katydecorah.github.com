{
  "name": "katy-decorah",
  "version": "0.0.0",
  "description": "Katy DeCorah's personal website",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "test-content": "extra-eyes {adventures,code,epicurean,notes}/_posts --dict=katydecorah.dic",
    "proofer": "htmlproofer ./_site",
    "lint-md": "remark {adventures,code,epicurean,notes}/_posts",
    "precommit": "lint-staged",
    "scsslint": "stylelint _sass/*.scss",
    "send-images": "node bin/appropriate-images.js"
  },
  "devDependencies": {
    "aws-sdk": "^2.478.0",
    "babel-eslint": "^8.2.6",
    "dotenv": "^7.0.0",
    "eslint": "^4.19.1",
    "extra-eyes": "1.0.1",
    "husky": "^0.14.3",
    "js-yaml": "^3.13.1",
    "lint-staged": "^7.3.0",
    "minimist": "^1.2.0",
    "moment": "^2.23.0",
    "prettier": "^1.18.2",
    "remark-cli": "^6.0.1",
    "remark-lint-no-dead-urls": "^0.4.1",
    "request": "^2.88.0",
    "retext-spell": "^2.4.1",
    "rimraf": "^2.6.3",
    "stylelint": "^9.9.0",
    "stylelint-scss": "^3.8.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.2",
    "underscore": "^1.9.1"
  },
  "optionalDependencies": {
    "@mapbox/appropriate-images": "^2.0.0"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --single-quote --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.yml": [
      "prettier --write",
      "git add"
    ],
    "**/*.scss": [
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@mapbox/appropriate-images": "^2.0.0"
  },
  "main": "index.js",
  "bin": {
    "katy-decorah": "appropriate-images.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/katydecorah.github.io.git"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katydecorah/katydecorah.github.io/issues"
  },
  "homepage": "https://github.com/katydecorah/katydecorah.github.io#readme"
}
