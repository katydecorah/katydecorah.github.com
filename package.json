{
  "name": "katy-decorah",
  "version": "0.0.0",
  "description": "Katy DeCorah's personal website",
  "scripts": {
    "test": "tape test/*.test.js | tap-min && npm run lint-md",
    "test-content": "extra-eyes {adventures,code,epicurean,notes}/_posts --dict=katydecorah.dic",
    "proofer": "htmlproofer ./_site",
    "lint-md": "remark {adventures,code,epicurean,notes}/_posts",
    "precommit": "lint-staged",
    "scsslint": "stylelint _sass/*.scss",
    "send-images": "node bin/appropriate-images.js"
  },
  "devDependencies": {
    "@double-great/remark-lint-alt-text": "^0.2.1",
    "babel-eslint": "^8.2.6",
    "eslint": "^6.8.0",
    "extra-eyes": "^1.1.1",
    "feather-icons": "^4.26.0",
    "husky": "^0.14.3",
    "js-yaml": "^3.13.1",
    "lint-staged": "^7.3.0",
    "moment": "^2.23.0",
    "prettier": "^1.19.1",
    "remark-cli": "^6.0.1",
    "remark-lint-no-dead-urls": "^0.4.1",
    "retext-spell": "^2.4.1",
    "stylelint": "^9.9.0",
    "stylelint-scss": "^3.14.2",
    "tap-min": "^2.0.0",
    "tape": "^4.13.2",
    "underscore": "^1.9.2"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --single-quote --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.yml": [
      "prettier --write",
      "git add"
    ],
    "**/*.scss": [
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "main": "index.js",
  "bin": {
    "update-icons": "bin/icons.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/katydecorah.github.io.git"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katydecorah/katydecorah.github.io/issues"
  },
  "homepage": "https://github.com/katydecorah/katydecorah.github.io#readme"
}
