{
  "name": "katy-decorah",
  "version": "0.0.0",
  "description": "Katy DeCorah's personal webiste",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "test-content": "editor {adventures,code,epicurean,notes}/_posts",
    "proofer": "htmlproofer ./_site",
    "precommit": "lint-staged",
    "scsslint": "stylelint _sass/*.scss"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "js-yaml": "^3.12.1",
    "lint-staged": "^7.3.0",
    "meow": "^5.0.0",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "prettier": "^1.16.4",
    "rehype-parse": "^6.0.0",
    "rehype-retext": "^2.0.2",
    "remark-frontmatter": "^1.3.1",
    "remark-message-control": "^4.1.1",
    "remark-parse": "^6.0.3",
    "remark-retext": "^3.1.2",
    "request": "^2.88.0",
    "retext-diacritics": "^1.2.2",
    "retext-english": "^3.0.2",
    "retext-indefinite-article": "^1.1.5",
    "retext-passive": "^1.3.2",
    "retext-redundant-acronyms": "^1.2.2",
    "retext-repeated-words": "^1.2.2",
    "retext-simplify": "^4.1.2",
    "stylelint": "^9.10.1",
    "stylelint-scss": "^3.5.2",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.2",
    "underscore": "^1.9.1",
    "unified": "^7.1.0",
    "unified-diff": "^2.0.0",
    "unified-engine": "^6.0.1",
    "update-notifier": "^2.5.0",
    "vfile-reporter": "^5.1.1"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --single-quote --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "**/*.yml": [
      "prettier --write",
      "git add"
    ],
    "**/*.scss": [
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "bin": {
    "editor": "./lib/editor/cli.js"
  },
  "dependencies": {}
}
